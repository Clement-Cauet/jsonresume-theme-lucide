{{#if startDate}}
  <code class="date">
    {{#if endDate}}
      {{#if (eq (MY startDate) (MY endDate))}}
        <span>{{MY startDate}}</span>
      {{else}}
        <span>{{MY startDate}} - {{MY endDate}}</span>
      {{/if}}
    {{else}}
      <span>{{MY startDate}} - Aujourd'hui</span>
    {{/if}}
  </code>
  <code class="date">
    <span>{{duration startDate endDate}}</span>
  </code>
{{/if}}