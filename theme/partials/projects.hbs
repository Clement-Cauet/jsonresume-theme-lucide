{{#if resume.projects.length}}
<section id="projects" class="section section-projects">
    <h2 class="section-title content-title">Projects</h2>
    {{#each resume.projects}}
    <section class="section-item">
        <div id="projects-header" class="section-header">
            <div>
                {{> date startDate=startDate endDate=endDate }}
            </div>
            <div>
                {{#if name}}
                <h3 class="section-subtitle">{{name}}</h3>
                {{/if}}
                <div class="section-information">
                    {{#if url}}
                    <a target="_blank" href="{{url}}">{{url}}</a>
                    {{/if}}
                    {{#if location}}
                    <span class="location">
                        <i class="fa-solid fa-location-dot"></i>
                        {{location}}
                    </span>
                    {{/if}}
                </div>
            </div>
        </div>
        <div id="projects-container" class="section-container">
            {{#if summary}}
            <div class="summary">{{paragraphSplit summary}}</div>
            {{/if}}
            {{#if highlights.length}}
            <ul class="highlights">
                {{#each highlights}}
                <li>{{paragraphSplit .}}</li>
                {{/each}}
            </ul>
            {{/if}}
            {{#if keywords.length}}
            <ul class="keywords">
                {{#each keywords}}
                <li>{{.}}</li>
                {{/each}}
            </ul>
            {{/if}}
        </div>
    </section>
    {{/each}}
</section>
{{/if}}